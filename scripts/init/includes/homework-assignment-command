
\newcommand{\SOURCEproblem}[PROBDEPTH][]{%
  \item[MIDSTRING]%
    \subimport{DOCDEPTHSTRINGproblems/SOURCEENDSTRING/}{statement}%
    \ifblank{#1}{%
    }{%
      \begin{itemize}%
        \foreach \x in {#1} {%
          \item[(\x)] \subimport{DOCDEPTHSTRINGproblems/SOURCEENDSTRING/\x/}%
                                {statement}%
        }%
      \end{itemize}%
    }%
}
